{
  "name": "@tracerbench/stats",
  "version": "6.1.1",
  "description": "Stats class written in TS-Node",
  "keywords": [
    "statistics",
    "stats",
    "tracerbench"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TracerBench/tracerbench.git"
  },
  "license": "BSD-2-Clause",
  "author": "https://github.com/lynchbomb",
  "files": [
    "dist",
    "!dist/test",
    "src"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "lint": "yarn eslint -c .eslintrc.js --ext .ts .",
    "test": "yarn lint && yarn nyc --extension .ts mocha \"test/*.test.ts\"",
    "build": "rm -rf ./dist && yarn tsc -b && yarn lint",
    "watch": "yarn tsc -b -w",
    "prepare": "yarn build",
    "api-docs": "yarn build && rm -rf ./etc && mkdir ./etc && yarn api-extractor run --local --verbose && yarn api-documenter markdown -i temp"
  },
  "dependencies": {
    "d3-array": "2.12.1",
    "d3-scale": "3.3.0",
    "fs-extra": "^10.0.0",
    "jstat": "^1.9.4",
    "path": "0.12.7",
    "tmp": "^0.2.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/d3-array": "^2.9.0",
    "@types/d3-scale": "^4.0.2",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.3.3",
    "@types/tmp": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "chai": "^4.3.4",
    "chai-files": "^1.4.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-oclif": "^0.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "mocha": "^9.0.2",
    "mock-fs": "^5.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "typescript-json-schema": "^0.52.0"
  },
  "engine": "node >= 10"
}
