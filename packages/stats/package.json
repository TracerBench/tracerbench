{
  "name": "@tracerbench/stats",
  "version": "7.0.0",
  "description": "Stats class written in TS-Node",
  "keywords": [
    "statistics",
    "stats",
    "tracerbench"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TracerBench/tracerbench.git"
  },
  "license": "BSD-2-Clause",
  "author": "https://github.com/lynchbomb",
  "files": [
    "dist",
    "!dist/test",
    "src"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "lint": "yarn eslint . --ext .ts && yarn tsc --noEmit",
    "test": "yarn lint && yarn nyc mocha",
    "build": "yarn tsc -b",
    "watch": "yarn tsc -b -w",
    "prepare": "yarn build",
    "api-docs": "yarn build && rm -rf ./etc && mkdir ./etc && yarn api-extractor run --local --verbose && yarn api-documenter markdown -i temp"
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "dependencies": {
    "d3-array": "~3.2.0",
    "d3-scale": "~3.3.0",
    "fs-extra": "^10.0.0",
    "jstat": "^1.9.5",
    "path": "0.12.7",
    "tmp": "^0.2.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/d3-array": "~2.0.0",
    "@types/d3-scale": "~3.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.12",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "chai": "^4.3.6",
    "chai-files": "^1.4.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "mocha": "9.1.4",
    "mock-fs": "^5.1.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^10.4.0",
    "typescript": "4.2.4",
    "typescript-json-schema": "^0.52.0"
  },
  "engine": "node >= 16"
}
