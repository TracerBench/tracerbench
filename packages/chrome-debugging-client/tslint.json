{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-floating-promises": true,
    "no-duplicate-imports": true,
    "no-import-side-effect": true,
    "arrow-parens": false,
    "no-conditional-assignment": false
  },
  "rulesDirectory": []
}
