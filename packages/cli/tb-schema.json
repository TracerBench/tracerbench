{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": {
    },
    "definitions": {
        "IBenchmarkEnvironmentOverride": {
            "additionalProperties": {
            },
            "properties": {
                "cpuThrottle": {
                    "type": "number"
                },
                "emulateDevice": {
                    "type": "string"
                },
                "emulateDeviceOrientation": {
                    "type": "string"
                },
                "network": {
                    "enum": [
                        "2g",
                        "3g",
                        "4g",
                        "FIOS",
                        "LTE",
                        "cable",
                        "dialup",
                        "dsl",
                        "edge",
                        "em-3g",
                        "fast-3g",
                        "none",
                        "offline",
                        "slow-3g"
                    ],
                    "type": "string"
                }
            },
            "type": "object"
        },
        "IHARServer": {
            "properties": {
                "dist": {
                    "type": "string"
                },
                "har": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "socksPort": {
                    "type": "number"
                },
                "url": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "IMarker": {
            "properties": {
                "label": {
                    "description": "Label of phase",
                    "type": "string"
                },
                "start": {
                    "description": "performance.mark name",
                    "type": "string"
                }
            },
            "type": "object"
        }
    },
    "properties": {
        "appName": {
            "type": "string"
        },
        "browserArgs": {
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "controlBenchmarkEnvironment": {
            "$ref": "#/definitions/IBenchmarkEnvironmentOverride"
        },
        "controlURL": {
            "type": "string"
        },
        "cpuThrottleRate": {
            "type": [
                "string",
                "number"
            ]
        },
        "debug": {
            "type": "boolean"
        },
        "emulateDevice": {
            "type": "string"
        },
        "emulateDeviceOrientation": {
            "type": "string"
        },
        "event": {
            "type": "string"
        },
        "experimentBenchmarkEnvironment": {
            "$ref": "#/definitions/IBenchmarkEnvironmentOverride"
        },
        "experimentURL": {
            "type": "string"
        },
        "extends": {
            "type": "string"
        },
        "fidelity": {
            "anyOf": [
                {
                    "enum": [
                        "high",
                        "low",
                        "medium",
                        "test"
                    ],
                    "type": "string"
                },
                {
                    "type": "number"
                }
            ]
        },
        "filter": {
            "type": "string"
        },
        "headless": {
            "type": "boolean"
        },
        "inputFilePath": {
            "type": "string"
        },
        "iterations": {
            "type": [
                "string",
                "number"
            ]
        },
        "json": {
            "type": "boolean"
        },
        "locations": {
            "type": "string"
        },
        "markers": {
            "anyOf": [
                {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                {
                    "items": {
                        "$ref": "#/definitions/IMarker"
                    },
                    "type": "array"
                },
                {
                    "items": {
                        "enum": [
                            "connectEnd",
                            "connectStart",
                            "decodedBodySize",
                            "domComplete",
                            "domContentLoadedEventEnd",
                            "domContentLoadedEventStart",
                            "domInteractive",
                            "domainLookupEnd",
                            "domainLookupStart",
                            "duration",
                            "encodedBodySize",
                            "entryType",
                            "fetchStart",
                            "initiatorType",
                            "loadEventEnd",
                            "loadEventStart",
                            "name",
                            "nextHopProtocol",
                            "redirectCount",
                            "redirectEnd",
                            "redirectStart",
                            "requestStart",
                            "responseEnd",
                            "responseStart",
                            "secureConnectionStart",
                            "startTime",
                            "toJSON",
                            "transferSize",
                            "type",
                            "unloadEventEnd",
                            "unloadEventStart",
                            "workerStart"
                        ],
                        "type": "string"
                    },
                    "type": "array"
                },
                {
                    "type": "string"
                }
            ]
        },
        "methods": {
            "type": "string"
        },
        "network": {
            "enum": [
                "2g",
                "3g",
                "4g",
                "FIOS",
                "LTE",
                "cable",
                "dialup",
                "dsl",
                "edge",
                "em-3g",
                "fast-3g",
                "none",
                "offline",
                "slow-3g"
            ],
            "type": "string"
        },
        "outputFilePath": {
            "type": "string"
        },
        "plotTitle": {
            "type": "string"
        },
        "regressionThreshold": {
            "type": [
                "string",
                "number"
            ]
        },
        "report": {
            "type": "string"
        },
        "runtimeStats": {
            "type": "boolean"
        },
        "servers": {
            "additionalItems": {
                "anyOf": [
                    {
                        "$ref": "#/definitions/IHARServer"
                    },
                    {
                        "$ref": "#/definitions/IHARServer"
                    }
                ]
            },
            "items": [
                {
                    "$ref": "#/definitions/IHARServer"
                },
                {
                    "$ref": "#/definitions/IHARServer"
                }
            ],
            "minItems": 2,
            "type": "array"
        },
        "socksPorts": {
            "additionalItems": {
                "anyOf": [
                    {
                        "type": "number"
                    },
                    {
                        "type": "number"
                    }
                ]
            },
            "items": [
                {
                    "type": "number"
                },
                {
                    "type": "number"
                }
            ],
            "minItems": 2,
            "type": "array"
        },
        "tbResultsFolder": {
            "type": "string"
        },
        "traceFrame": {
            "type": "string"
        },
        "tracingLocationSearch": {
            "type": "string"
        },
        "url": {
            "type": "string"
        }
    },
    "type": "object"
}

