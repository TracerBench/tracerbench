{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "trailing-comma": false,
    "no-floating-promises": true,
    "no-shadowed-variable": false,
    "no-var-requires": false,
    "quotemark": false,
    "max-line-length": [true, 160],
    "member-ordering": false
  },
  "rulesDirectory": []
}
