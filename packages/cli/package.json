    "cli-table3": "^0.5.1",
    "d3-array": "^2.0.3",
    "d3-scale": "^3.0.0",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.2.2",
    "json-query": "^2.2.2",
    "listr": "^0.14.3",
    "path": "0.12.7",
    "tracerbench": "^1.0.0",
    "tslib": "^1.9.3",
    "wilson-interval": "^4.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.0",
    "@oclif/test": "^1.2.2",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/chai-fs": "^2.0.1",
    "@types/d3-scale": "^2.1.1",
    "@types/chalk": "^2.2.0",
    "@types/d3-array": "^1.2.6",
    "@types/fs-extra": "^5.0.4",
    "@types/inquirer": "^0.0.44",
    "@types/json-query": "^2.2.0",
    "@types/listr": "^0.13.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "chai": "^4.2.0",
    "chai-fs": "^2.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.4000"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "tracerbench",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "yarn build && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha \"test/commands/**/*.test.ts\" \"test/helpers/**/*.test.ts\"",
    "test:helpers": "nyc --extension .ts mocha \"test/helpers/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "build": "rm -rf dist && tsc -b"
  },
  "gitHead": "95ab52895412d32c451c211463521dda5ac782e3"
}