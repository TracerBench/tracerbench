{
  "name": "chrome-tracing",
  "version": "0.10.0",
  "description": "Parser for chrome traces",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "tsc && cp ./{report.R,plot.R} ./dist/",
    "build": "tsc",
    "compile": "tsc --watch",
    "lint": "tslint -p tsconfig.json",
    "test": "tsc && node dist/test/build.js && node dist/test.js",
    "test:only": "node dist/test.js",
    "test:build": "node dist/test/build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krisselden/chrome-tracing.git"
  },
  "files": [
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.js.map",
    "dist/lib"
  ],
  "keywords": [
    "chrome",
    "tracing"
  ],
  "author": "Kris Selden <kris.selden@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^7.0.23",
    "@types/uglify-js": "^2.6.28",
    "bower": "^1.7.9",
    "tslint": "^5.4.2",
    "typescript": "^2.3.4",
    "uglify-js": "^2.7.0"
  },
  "dependencies": {
    "array-binsearch": "^1.0.1",
    "chrome-debugging-client": "^0.4.1"
  },
  "engine": "node >= 4.0.0"
